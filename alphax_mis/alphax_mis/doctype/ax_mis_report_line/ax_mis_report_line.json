{
  "doctype": "DocType",
  "name": "AX MIS Report Line",
  "module": "AlphaX MIS",
  "istable": 1,
  "editable_grid": 1,
  "fields": [
    {
      "fieldname": "line_code",
      "label": "Line Code",
      "fieldtype": "Data",
      "reqd": 1
    },
    {
      "fieldname": "title",
      "label": "Title",
      "fieldtype": "Data",
      "reqd": 1
    },
    {
      "fieldname": "line_type",
      "label": "Line Type",
      "fieldtype": "Select",
      "options": "Group\nFlag\nFormula",
      "reqd": 1
    },
    {
      "fieldname": "flag",
      "label": "Flag",
      "fieldtype": "Link",
      "options": "AX MIS Flag",
