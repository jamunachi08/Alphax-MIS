    {
      "fieldname": "formula",
      "label": "Formula",
      "fieldtype": "Data",
      "depends_on": "eval:doc.line_type=='Formula'"
    },
    {
      "fieldname": "indent",
      "label": "Indent",
      "fieldtype": "Int",
      "default": 0
    },
    {
      "fieldname": "bold",
      "label": "Bold",
      "fieldtype": "Check",
      "default": 0
    }
  ]
